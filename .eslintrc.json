{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "prettier",
        "plugin:@typescript-eslint/recommended",
        "airbnb-base"
    ],
    "plugins": [
        "prettier",
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "@typescript-eslint/rule-name": "error",
        "no-console": "error"
    }
}